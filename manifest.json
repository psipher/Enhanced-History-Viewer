{
  "manifest_version": 3,
  "name": "Enhanced History Viewer",
  "version": "1.1",
  "description": "View your Chrome browsing history with full URLs",
  "permissions": ["history", "favicon"],
  "host_permissions": ["https://www.google.com/s2/*"],
  "chrome_url_overrides": {
    "history": "history.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}

